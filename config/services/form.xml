<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="App\Form\Extension\Admin\ProductTypeExtension">
            <tag name="form.type_extension" extended-type="Sylius\Bundle\ProductBundle\Form\Type\ProductType" priority="100" />
        </service>

        <service id="App\Form\Type\ProductColorType">
            <argument type="service" id="App\Service\ProductColorsInterface" />
            <tag name="form.type" />
        </service>
    </services>
</container>
